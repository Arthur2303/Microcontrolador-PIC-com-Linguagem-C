#include <18F4550.h>
#fuses XT, NOWDT, NOMCLR
#use delay(clock=4MHz)

#INT1_EXT//Diretiva da função abaixo
void int_externa1() {
   output_toggle(PIN_D6);
}

void main () {

   enable_interrupts(GLOBAL);
   enable_interrupts(INT1_EXT);
   
   while(true) {
      
      output_toggle(PIN_D7);
      delay_ms(500);
      
   }

}






